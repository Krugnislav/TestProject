<table class="noborder">
    <tr>
        <td rowspan="2">ID: {{user.ID}}<input type="hidden" name="id" value="{{user.ID}}"></td>
        <td>Firstname:</td>
        <td><input type="text" name="fn" class="w100" value="{{user.Name}}"></td>
        <td>Description: </td>
        <td>Email:</td>
        <td><input type="text" name="em" class="w180" value="{{user.Email}}"></td>
        <td><input type="button" value=" save " onclick="alert('Save :)')"></td>
    </tr>
    <tr>
        <td>Lastname:</td>
        <td><input type="text" name="ln" class="w100" value="{{user.LastName}}"></td>
        <td><input type="button" value=" cancel" ng-click="setEditId(-1)"></td>
    </tr>
</table>
